{
    "name": "vite-vue-electron-template",
    "version": "0.0.0",
    "authors": [
        "Cladonia Frontend Development Team",
        "Hydrogen <mozaisoft@foxmail.com>"
    ],
    "license": "MIT",
    "private": true,
    "main": "dist/main/index.js",
    "scripts": {
        "lint": "eslint src/*",
        "lint-fix": "eslint --fix src/*",
        "build": "vite build",
        "dev": "vue-tsc --noEmit && vite",
        "dist:all": "electron-builder -c ./electron-builder.json",
        "dist:win": "electron-builder -w -c ./electron-builder.json",
        "dist:mac": "electron-builder -m -c ./electron-builder.json",
        "dist:linux": "electron-builder -l -c ./electron-builder.json",
        "dist:win:x64": "electron-builder -w --x64 -c ./electron-builder.json",
        "dist:mac:x64": "electron-builder -m --x64 -c ./electron-builder.json",
        "dist:linux:x64": "electron-builder -l --x64 -c ./electron-builder.json",
        "dist:win:ia32": "electron-builder -w --ia32 -c ./electron-builder.json",
        "dist:mac:ia32": "electron-builder -m --ia32 -c ./electron-builder.json",
        "dist:linux:ia32": "electron-builder -l --ia32 -c ./electron-builder.json",
        "dist:win:armv7l": "electron-builder -w --armv7l -c ./electron-builder.json",
        "dist:mac:armv7l": "electron-builder -m --armv7l -c ./electron-builder.json",
        "dist:linux:armv7l": "electron-builder -l --armv7l -c ./electron-builder.json",
        "dist:win:arm64": "electron-builder -w --arm64 -c ./electron-builder.json",
        "dist:mac:arm64": "electron-builder -m --arm64 -c ./electron-builder.json",
        "dist:linux:arm64": "electron-builder -l --arm64 -c ./electron-builder.json",
        "postinstall": "electron-builder install-app-deps"
    },
    "devDependencies": {
        "@emotion/css": "^11.7.1",
        "@rollup/plugin-alias": "^3.1.9",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-eslint": "^8.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-replace": "^3.0.1",
        "@types/mime": "^2.0.3",
        "@types/node": "^17.0.10",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "@vitejs/plugin-vue": "^2.0.1",
        "@vitejs/plugin-vue-jsx": "^1.3.3",
        "chokidar": "^3.5.3",
        "electron-builder": "^22.14.5",
        "esbuild": "^0.14.11",
        "eslint": "^7.12.1",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-vue": "^8.3.0",
        "execa": "^6.0.0",
        "filesize": "^8.0.7",
        "fs-extra": "^10.0.0",
        "mime": "^3.0.0",
        "picocolors": "^1.0.0",
        "pinia": "^2.0.9",
        "rollup": "2",
        "rollup-plugin-esbuild": "^4.8.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4",
        "uuid": "^8.3.2",
        "vite": "^2.7.12",
        "vite-plugin-eslint": "^1.3.0",
        "vue": "^3.2.27",
        "vue-router": "^4.0.12",
        "vue-tsc": "^0.30.6"
    },
    "dependencies": {
        "electron": "16.0.6"
    }
}