{
    "asar": true,
    "extraFiles": [],
    "publish": [{
        "provider": "generic",
        "url": "http://127.0.0.1"
    }],
    "productName": "ViteApplication",
    "appId": "io.cladonia.vite.app",
    "directories": {
        "output": "release"
    },
    "files": [
        "dist/**/*"
    ],
    "dmg": {
        "contents": [{
                "x": 410,
                "y": 150,
                "type": "link",
                "path": "/Applications"
            },
            {
                "x": 130,
                "y": 150,
                "type": "file"
            }
        ]
    },
    "mac": {
        "icon": "resources/icons/icon.icns"
    },
    "win": {
        "icon": "resources/icons/icon.ico",
        "target": "nsis"
    },
    "linux": {
        "target": "deb",
        "icon": "resources/icons"
    }
}